    "@c8y/ngx-components": "1006.2.0",
    "core-js": "^3.4.0",
    "jszip": "^3.4.0",
    "ngx-bootstrap": "5.5.0",
    "rxjs": "~6.4.0",
    "url-search-params-polyfill": "6.0.0",
    "zone.js": "~0.9.1",
    "@angular-devkit/build-angular": "0.803.17",
    "@angular/compiler-cli": "8.2.13",
    "@angular/language-service": "8.2.13",
    "@c8y/cli": "1006.2.0",
    "typescript": "3.5.3",
    "webpack-external-import": "^2.2.3",
    "ng-packagr": "^9.1.1"
  }
